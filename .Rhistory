skew <-function(x,p) {
k = length(x)
m = sum(x*p)
v = sum( (x-m)^2 *p)
m3 = sum( (x-m)^3 *p)
sk = m3/v^1.5
return(sk)
}
kurt <-function(x,p) {
k = length(x)
m = sum(x*p)
v = sum( (x-m)^2 *p)
m4 = sum( (x-m)^4 *p)
k = m4/v^2
return(k)
}
